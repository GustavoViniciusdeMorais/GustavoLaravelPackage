{
    "name": "gustavovinicius/blogpackage",
    "description": "A demo package",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Gustavo Vinicius",
        "email": "gustavoviniciusmoraisti@gmail.com"
      }
    ],
    "autoload": {
        "psr-4": {
            "GustavoVinicius\\BlogPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GustavoVinicius\\BlogPackage\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GustavoVinicius\\BlogPackage\\BlogPackageServiceProvider"
            ],
            "aliases": {
                "Calculator": "GustavoVinicius\\BlogPackage\\Facades\\Calculator"
            }
        },
        "installer-paths":{
            "./packages/": ["gustavo"]
        }
    },
    "version": "1.0.1"
}
